{
  "expo": {
    "name": "Aora",
    "slug": "aora",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/AppLogo.png",
    "scheme": "aora",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/images/splashScreen.png",
      "resizeMode": "contain",
      "backgroundColor": "white"
    },
    "ios": {
      "supportsTablet": true,
      "package": "com.jsm.aora",
      "bundleIdentifier": "com.jsm.aora"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/AppLogo.png",
        "backgroundColor": "red"
      },
      "package": "com.jsm.aora"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "eas": {
        "projectId": "d7355ab4-de3b-4059-a6ce-12366838fa42"
      }
    }
  }
}